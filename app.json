{
  "expo": {
    "name": "honey-badgers",
    "slug": "honey-badgers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "./assets/fonts/"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alobar.honeybadgers"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alobar.honeybadgers"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ef1d42b5-442f-4568-8ce8-8b7e1a8307ab"
      }
    }
  }
}
